{% extends '_layouts/base' %}


{% do craft.entryCount.increment(entry.id) %}

{% block content %}


  {% if entry.type == "general" %}

    {% include "_includes/post-templates/general" %}

  {% elseif entry.type == "accordion" %}

    {% do craft.app.elements.eagerLoadElements(
      className(entry),
      [entry],
      ['multiParagraph']
    ) %}
    {% include "_includes/post-templates/accordion" %}

  {% endif %}

  
{% endblock %}